<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(googlemap) -%>">
  <span class='title'>
    <%= googlemap.address %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.googlemaps_googlemap_path(googlemap),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_googlemaps_admin_googlemap_path(googlemap),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.googlemaps_admin_googlemap_path(googlemap),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => googlemap.address),
        :method => :delete %>
  </span>
</li>
