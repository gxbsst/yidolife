<ul>
  <% if ::Refinery::Googlemaps::Admin::GooglemapsController.searchable? %>
    <li>
      <%= render '/refinery/admin/search', :url => refinery.googlemaps_admin_googlemaps_path %>
    </li>
  <% end %>
  <li>
    <%= link_to t('.create_new'), refinery.new_googlemaps_admin_googlemap_path,
                 :class => "add_icon" %>
  </li>
<% if !searching? && ::Refinery::Googlemaps::Admin::GooglemapsController.sortable? && ::Refinery::Googlemaps::Googlemap.any? %>
  <li>
    <%= link_to t('.reorder', :what => "Googlemaps"),
                 refinery.googlemaps_admin_googlemaps_path,
                 :id => "reorder_action",
                 :class => "reorder_icon" %>

    <%= link_to t('.reorder_done', :what => "Googlemaps"),
                 refinery.googlemaps_admin_googlemaps_path,
                 :id => "reorder_action_done",
                 :style => "display: none;",
                 :class => "reorder_icon" %>
  </li>
<% end %>
</ul>
